{
  "name": "@jrmc/adonis-unpoly",
  "version": "1.0.2",
  "type": "module",
  "description": "Provider and middleware unpoly for adonisjs",
  "engines": {
    "node": ">=20.12.0"
  },
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/batosai/adonis-unpoly.git"
  },
  "author": "Jeremy Chaufourier jeremy@chaufourier.fr",
  "license": "MIT",
  "keywords": [
    "providers",
    "middleware",
    "unpoly",
    "adonisjs"
  ],
  "files": [
    "build"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "format": "prettier --write ."
  },
  "exports": {
    ".": "./build/index.js",
    "./unpoly_provider": "./build/providers/unpoly_provider.js",
    "./unpoly_middleware": "./build/src/unpoly_middleware.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.17.1",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@types/node": "^22.10.7",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "volta": {
    "node": "20.12.0"
  }
}
